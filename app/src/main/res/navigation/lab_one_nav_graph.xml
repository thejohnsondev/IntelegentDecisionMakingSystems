<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/lab_one_nav_graph"
    app:startDestination="@id/labOneHomeFragment">

    <fragment
        android:id="@+id/labOneHomeFragment"
        android:name="com.thejohnsondev.intelegentdecisionmakingsystems.lab1.presentation.first_step.FirstStepFragment"
        android:label="LabOneHomeFragment" >
        <action
            android:id="@+id/action_labOneHomeFragment_to_secondStepFragment"
            app:destination="@id/secondStepFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondStepFragment"
        android:name="com.thejohnsondev.intelegentdecisionmakingsystems.lab1.presentation.second_step.SecondStepFragment"
        android:label="SecondStepFragment" >
        <action
            android:id="@+id/action_secondStepFragment_to_thirdStepFragment"
            app:destination="@id/thirdStepFragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdStepFragment"
        android:name="com.thejohnsondev.intelegentdecisionmakingsystems.lab1.presentation.third_step.ThirdStepFragment"
        android:label="ThirdStepFragment" >
        <action
            android:id="@+id/action_thirdStepFragment_to_resultStepFragment"
            app:destination="@id/resultStepFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultStepFragment"
        android:name="com.thejohnsondev.intelegentdecisionmakingsystems.lab1.presentation.result_step.ResultStepFragment"
        android:label="ResultStepFragment" />
</navigation>